{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildType": "app"
      },
      "distribution": "internal",
      "channel": "preview",
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
